{
    "version": 2,
    "devCommand": "npm run start:server",
    "builds": [
      {
        "src": "build/index.ts",
        "use": "@vercel/node",
        "config": { "includeFiles": ["build/**"] }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "build/index.ts"
      }
    ]
}